    "lint-staged": "^9.2.5",
    "ms": "^2.1.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "nps": "^5.9.8",
    "nps-utils": "^1.7.0",
    "nyc": "^14.1.1",
    "open": "^6.4.0",
    "postcss": "^7.0.18",
    "postcss-font-magician": "^2.2.2",
    "postcss-font-smoothing": "^0.1.0",
